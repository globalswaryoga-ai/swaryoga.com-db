# Local development environment variables (safe placeholders)
# Do NOT commit real secrets.

# MongoDB connection string
MONGODB_URI="mongodb+srv://<username>:<password>@<cluster>/<db>?retryWrites=true&w=majority"

# JWT secret used for signing/verifying user tokens
JWT_SECRET="replace_me_with_a_long_random_string"

# Admin panel auth token (used by admin APIs). You may keep the default in code,
# but setting this makes it explicit.
ADMIN_PANEL_TOKEN="admin_replace_me"
